/var/log/mqtt_client.log {
        weekly
        size=10M
        rotate 4
        compress
        nodelaycompress
        missingok
        notifempty
}

/var/log/mqtt_client.error.log {
        weekly
        size=10M
        rotate 4
        compress
        nodelaycompress
        missingok
        notifempty
}
